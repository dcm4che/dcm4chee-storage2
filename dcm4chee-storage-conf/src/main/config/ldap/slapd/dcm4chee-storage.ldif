# DICOM Application Configuration Data Model Hierarchy LDAP Schema
dn: cn=dcm4chee-storage,cn=schema,cn=config
objectClass: olcSchemaConfig
cn: dcm4chee-storage
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.1 NAME 'dcmProviderName'
  DESC 'Provider Name'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.2 NAME 'dcmStorageSystemGroupID'
  DESC 'Storage System Group ID'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.3 NAME 'dcmStorageSystemID'
  DESC 'Storage System ID'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.4 NAME 'dcmNextStorageSystemID'
  DESC 'Next Storage System ID'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.5 NAME 'dcmActiveStorageSystemID'
  DESC 'Next Storage System ID'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.6 NAME 'dcmStorageSystemPath'
  DESC 'Storage System ID'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.7 NAME 'dcmStorageSystemStatus'
  DESC 'Storage System Status: OK, FULL or NOT_ACCESSABLE. OK if absent'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.8 NAME 'dcmStorageReadOnly'
  DESC 'Indicates if Storage is read-only. false if absent'
  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.9 NAME 'dcmStorageAccessTime'
  DESC 'Storage Access Time. 0 if absent'
  EQUALITY integerMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.27
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.10 NAME 'dcmStorageMinFreeSpace'
  DESC 'Threshold of free space of active Storage System in format <integer>{kB|MB|GB|KiB|MiB|GiB}.'
  EQUALITY caseExactIA5Match
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
  SINGLE-VALUE )
olcAttributeTypes: ( 1.2.40.0.13.1.15.30.3.11 NAME 'dcmStorageMountCheckFile'
  DESC 'Specifies path of file which appearance indicates a failed mount'
  EQUALITY caseIgnoreMatch
  SUBSTR caseIgnoreSubstringsMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.15
  SINGLE-VALUE )
olcObjectClasses: ( 1.2.40.0.13.1.15.30.4.1 NAME 'dcmStorageDeviceExtension'
  DESC 'Storage Device Extension'
  SUP top STRUCTURAL
  MUST (
    cn ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.30.4.2 NAME 'dcmStorageSystemGroup'
  DESC 'Storage System Group'
  SUP top STRUCTURAL
  MUST (
    dcmStorageSystemGroupID )
  MAY (
    dcmActiveStorageSystemID $
    dcmNextStorageSystemID $
    dicomInstalled) )
olcObjectClasses: ( 1.2.40.0.13.1.15.30.4.3 NAME 'dcmStorageSystem'
  DESC 'Storage System'
  SUP top STRUCTURAL
  MUST (
    dcmStorageSystemID $
    dcmStorageSystemPath $
    dcmStorageSystemStatus $
    dcmProviderName )
  MAY (
    dcmNextStorageSystemID $
    dcmStorageReadOnly $
    dcmStorageAccessTime $
    dcmStorageMinFreeSpace $
    dcmStorageMountCheckFile $
    dicomInstalled) )
olcObjectClasses: ( 1.2.40.0.13.1.15.30.4.4 NAME 'dcmStorageArchiver'
  DESC 'Storage Archiver'
  SUP top STRUCTURAL
  MUST (
    cn $
    dcmProviderName ) )
olcObjectClasses: ( 1.2.40.0.13.1.15.30.4.5 NAME 'dcmStorageFileCache'
  DESC 'Storage File Cache'
  SUP top STRUCTURAL
  MUST (
    cn $
    dcmProviderName ) )
